import java.util.*;
 class Rev
 {
    public static void main(String args[])
{
        int n,r,sum=0,temp;
        Scanner s = new Scanner(System.in);
        System.out.print("Enter a number: ");
        n = s.nextInt();
        temp=n
        while(n>0)
        r=n%10;
        sum=sum+r*r*r;
        n=n/10;
        if (n=temp) 
{
            System.out.println(n + " is an palindromr.");
        } 
else 
{
            System.out.println(n + " is not palindrome");
        }


    }
