import java.util.*;
public class Rev 
{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter an integer: ");
        int n = scanner.nextInt();
        if (n > 0)
 {
            while (n % 3 == 0) 
{
                n /= 3;
            }
}
if(n==1)
{
System.out.println("Output = " + true);
        }
else
{
       System.out.println("Output = " + false);

        }
    }
}

 
