import java.util.*;
 class Rev
 {
    public static void main(String args[])
{
        int n,r,sum=0;
        Scanner s = new Scanner(System.in);
        System.out.print("Enter a number: ");
        n = s.nextInt();
        while(n>0)
        r=n%10;
        sum=sum+r*r*r;
        n=n/10;
        if (n=sum) 
{
            System.out.println(n + " is an Armstrong number.");
        } 
else 
{
            System.out.println(n + " is not an Armstrong number.");
        }


    }
