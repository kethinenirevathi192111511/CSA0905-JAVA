import java.util.*;
 class Rev
 {
    public static void main(String args[]) 
    {
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the number of terms in the Fibonacci series: ");
        int n = s.nextInt();
        int a = 0;
        int b = 1;
        System.out.println("Fibonacci Series (First " + n + " terms):");
        for (int i = 1; i <= n; i++) {
            System.out.print(a + " ");
            int next = a+b;
            a=b;
            b = next;
        }
    }
}
